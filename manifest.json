{
  "name": "Spotify Widget",
  "description": "Spotify widget in chrome extensions",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": ["sidePanel"],
  "side_panel": {
    "default_path": "dist/sidepanel.html"
  },

  "action": {
    "default_title": "Click to open panel"
  },
  "background": {
    "service_worker": "service-worker.js"
  }
}
